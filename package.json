{
  "name": "students-points-app",
  "version": "1.0.0",
  "description": "",
  "main": "src/main/main.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "concurrently \"npm run start:renderer\" \"npm run start:electron\"",
    "start:renderer": "cross-env NODE_ENV=development webpack serve --config webpack.config.js",
    "start:electron": "wait-on http://localhost:3000 && cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:electron",
    "build:renderer": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "build:electron": "electron-builder",
    "build:win": "npm run build && electron-builder --win",
    "rebuild": "electron-rebuild -f -w better-sqlite3"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
