    "dotenv": "^17.2.3",
    "electron": "39.1.1",
    "electron-is-dev": "3.0.1",
    "exceljs": "4.4.0",
    "jsonwebtoken": "9.0.2",
    "prop-types": "15.8.1",
    "react": "19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.0",
    "react-router-dom": "7.9.5",
    "sql.js": "^1.13.0"
  },
  "devDependencies": {
    "@babel/core": "7.28.5",
